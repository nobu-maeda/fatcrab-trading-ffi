namespace FatcrabTrading {};

[Enum]
interface Auth {
  None();
  UserPass(string username, string password);
  Cookie(string file);
};

[Enum]
interface Network {
  Bitcoin();
  Testnet();
  Signet();
  Regtest();
};

interface Trader {
    constructor(string url, Auth auth, Network network);
    [Name=new_with_keys]
    constructor(string key, string url, Auth auth, Network network);
};